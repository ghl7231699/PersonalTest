<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.liangge.rxjavatest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.SchedulerActivity"/>
        <activity android:name=".ui.activity.MapActivity"/>
        <activity android:name=".ui.activity.AuthCodeActivity"/>
        <activity android:name=".ui.activity.baseactivity.BaseActivity"/>
        <activity android:name=".ui.activity.DaggerActivity"/>
        <activity
            android:name=".ui.activity.MaterialDesignActivity"
            android:label="@string/Fruits"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".ui.activity.FruitActivity"/>
        <activity android:name=".ui.activity.UserInfoActivity">
        </activity>
        <activity android:name=".ui.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>